<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FixedAspectRatioImageView">
        <attr name="aspectRatio" format="float" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundableImageView">
        <attr name="cornersRadius" format="dimension" />
        <attr name="circle" format="boolean" />
        <attr name="backgroundColor" format="color" />
        <attr name="r_border" format="boolean" />
        <attr name="r_borderWidth" format="dimension" />
        <attr name="r_borderColor" format="color" />
    </declare-styleable>


</resources>